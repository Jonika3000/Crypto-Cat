    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Images\CatCoin.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\CatCoin.png" />
    <None Remove="Images\Coins.png" />
    <None Remove="Images\Exchange.png" />
    <None Remove="Images\exclamation.png" />
    <None Remove="Images\GitHub.png" />
    <None Remove="Images\HomeIcon.png" />
